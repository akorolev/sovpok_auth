<% if resource.errors.count == 0 %>
        $('#form-place').foundation('reveal', 'close');
        top.location.href="<%= session[:user_return_to] %>";
        $("#form-place").empty();
<% else %>
        $("#form-place").empty();
        $("#form-place").append('<%= j render 'devise/sessions/new' %>');
<% end %>
